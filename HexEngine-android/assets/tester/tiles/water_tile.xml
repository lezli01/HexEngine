<!-- 
	This is an example tile, it can be stepped on
	and when a unit steps on it it will grant random
	strength between 1 and 10.
-->

<Tile
    name="Water Tile"
    description="@example_texts/@example_text"
    id="@water_tile"
    can_walk="true"
    graphics="@water_tile_graphics">
    
    <StatBuffs
        name="Example StatBuffs"
        description="@example_texts/@example_text"
        id="@example_statbuffs">
        
        <StatBuff
            stat="@strength"
            min_val="1"
            max_val="10"/>
        
    </StatBuffs>
    
</Tile>